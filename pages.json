{
	"easycom": {
		"autoscan": true,
		"tui-ui": "ThorUI文档 https://thorui.cn/doc/docs/tui-basic.html",
		"u-view": "uVieww文档 http://uviewui.com/components/intro.html",
		"l-painter": "canvas海报生成组件文档 http://liangei.gitee.io/limeui-docs/components/painter/  （此组件整合到自定义组件内   组件名称：gy-lime-painter）",
		"custom": {
			"tui-(.*)": "@/components/thorui/tui-$1/tui-$1.vue",
			"gy-(.*)": "@/components/gy-custom-ui/gy-$1/gy-$1.vue",
			"u-(.*)": "@/components/uview-ui/components/u-$1/u-$1.vue"
		}
	},
	"pages": [
		{
			"path": "pages/tabbar/rent-car/rent-car",
			"style": {
				"navigationBarTitleText": "租车", 
				"enablePullDownRefresh": false
			}
        },
        {
			"path": "pages/tabbar/put-up/put-up",
			"style": {
				"navigationBarTitleText": "住宿", 
				"enablePullDownRefresh": false
			}
        },
        {
			"path": "pages/tabbar/salary-earner/salary-earner",
			"style": {
				"navigationBarTitleText": "雇工", 
				"enablePullDownRefresh": false
			}
        },
        {
			"path": "pages/tabbar/preson-center/preson-center",
			"style": {
				"navigationBarTitleText": "我的", 
				"enablePullDownRefresh": false
			}
        }
    ],
    "tabBar": {
        "color": "#b1b7bf",
        "selectedColor": "#0877bc",
        "borderStyle": "black",
        "backgroundColor": "#ffffff",
        "list": [
            {
                "pagePath": "pages/tabbar/rent-car/rent-car",
                "iconPath": "static/images/icon/tabbar-1.png",
                "selectedIconPath": "static/images/icon/tabbar-1-s.png",
                "text": "租车"
            },
            {
                "pagePath": "pages/tabbar/put-up/put-up",
                "iconPath": "static/images/icon/tabbar-2.png",
                "selectedIconPath": "static/images/icon/tabbar-2-s.png",
                "text": "住宿"
            },
            {
                "pagePath": "pages/tabbar/salary-earner/salary-earner",
                "iconPath": "static/images/icon/tabbar-3.png",
                "selectedIconPath": "static/images/icon/tabbar-3-s.png",
                "text": "雇工"
            },
            {
                "pagePath": "pages/tabbar/preson-center/preson-center",
                "iconPath": "static/images/icon/tabbar-4.png",
                "selectedIconPath": "static/images/icon/tabbar-4-s.png",
                "text": "我的"
            }
        ]
    },
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "租车信息",
		"navigationBarBackgroundColor": "#fff",
		"backgroundColor": "#fff"
	},
	"condition": {
		"current": 1,
        "list": [ 
            {
                "name": "租车",
                "path": "pages/tabbar/rent-car/rent-car" 
            },
            {
                "name": "住宿",
                "path": "pages/tabbar/put-up/put-up" 
            },
            {
                "name": "雇工",
                "path": "pages/tabbar/salary-earner/salary-earner" 
            },
            {
                "name": "我的",
                "path": "pages/tabbar/preson-center/preson-center" 
            }
		]
	}
}
